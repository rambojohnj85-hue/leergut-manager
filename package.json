{
  "name": "leergutmanager",
  "version": "1.3.5",
  "description": "Leergut Manager Electron + Vite + React",
  "author": "Tobias Lohmann",
  "main": "main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "electron .",
    "electron:build": "electron-builder --win --x64 --publish never"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^28.3.3",
    "electron-builder": "^24.13.3",
    "vite": "^5.4.19"
  },
  "build": {
    "appId": "com.leergut.manager",
    "productName": "leergutmanager",
    "files": [
      "dist/**",
      "main.js",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    }
  }
}