{
  "name": "leergutmanager",
  "version": "1.2.0",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "electron:dev": "concurrently \"vite dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "electron-builder --win --x64 --publish never"
  },
  "devDependencies": {
    "electron": "^28.3.3",
    "electron-builder": "^24.13.3",
    "typescript": "^5.6.2",
    "vite": "^5.4.10",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "build": {
    "appId": "com.leergut.manager",
    "productName": "Leergut Manager",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist",
      "dist-electron"
    ],
    "extraMetadata": {
      "main": "dist-electron/main.js"
    },
    "win": {
      "icon": "public/icon.png",
      "target": "nsis"
    }
  }
}